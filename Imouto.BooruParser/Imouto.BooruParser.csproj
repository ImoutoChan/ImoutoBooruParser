<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.6</TargetFramework>
    <Authors>ImoutoChan</Authors>
    <Company>ImoutoChan</Company>
    <PackageId>Imouto.BooruParser</PackageId>
    <Product>Imouto.BooruParser</Product>
    <Description>The .net standart library for parsing data from booru sites (chan.sankakucomplex.com, danbooru.donmai.us, yande.re).

The library can parse information about individual posts, tags and notes history and can perform a search on this sites.</Description>
    <Copyright>(c) ImoutoChan</Copyright>
    <PackageProjectUrl>https://github.com/ImoutoChan/ImoutoBooruParser</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/ImoutoChan/ImoutoBooruParser/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/ImoutoChan/ImoutoBooruParser</RepositoryUrl>
    <PackageTags>booru search parser danbooru sankakuchannel yande.re</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.2.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="HtmlAgilityPack" Version="1.6.15" />
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
    <PackageReference Include="NLog" Version="4.5.0-rc04" />
  </ItemGroup>

</Project>
